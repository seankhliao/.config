[alias]
    a = add
    cam = commit -a -m
    camp = !git commit -a -m \"$1\" && git push && :
    cm = commit -m
    l = pull
    lr = pull --rebase
    lo = log --graph --color=always --format='%C(auto)%h%d %s %C( bold black)%cr %C(blue)<%an>'
    p = push
    puom = push -u origin master
    rao = remote add origin
    s = status

[blame]
    coloring = highlightRecent

[color]
    ui = always
    

[credential]
    helper = store


